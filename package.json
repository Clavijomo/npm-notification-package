{
  "version": "1.0.2",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/notify-hub.esm.js",
      "require": "./dist/notify-hub.cjs.production.min.js"
    }
  },
  "repository": {
    "url": "https://github.com/Clavijomo/npm-notification-package/tree/main",
    "type": "git"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "notify-hub",
  "author": "Clavijomo",
  "keywords": [
    "notification",
    "react",
    "custom",
    "notify",
    "package",
    "npm"
  ],
  "description": "Custom notifications, functional properties, and variation of types and styles",
  "module": "dist/notify-hub.esm.js",
  "size-limit": [
    {
      "path": "dist/notify-hub.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/notify-hub.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@chromatic-com/storybook": "^1.9.0",
    "@size-limit/preset-small-lib": "^11.1.6",
    "@storybook/addon-essentials": "^8.3.6",
    "@storybook/addon-interactions": "^8.3.6",
    "@storybook/addon-links": "^8.3.6",
    "@storybook/addon-onboarding": "^8.3.6",
    "@storybook/blocks": "^8.3.6",
    "@storybook/react": "^8.3.6",
    "@storybook/react-vite": "^8.3.6",
    "@storybook/test": "^8.3.6",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "husky": "^9.1.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rollup-plugin-postcss": "^4.0.2",
    "size-limit": "^11.1.6",
    "storybook": "^8.3.6",
    "tsdx": "^0.14.1",
    "tslib": "^2.8.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@mui/icons-material": "^6.1.5"
  }
}
